{
  "manifest_version": 2,

  "name": "Text Copy Plugin",
  "description": "This extension will save text",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html",
   "default_title": "Click to save url"
  },
  "permissions": [
   "activeTab","storage"       
   ]
}